<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	
     <class name="org.apachechina.fsnote.domain.Note" table="fsnote_Note">    
        <id name="id" column="Note_ID">
            <generator class="native"/>
        </id>        
        <property name="title" type="string"/>
        <property name="gut" type="string"/> 
        <property name="date" type="date"/>   
    </class>
    <class name="org.apachechina.fsnote.domain.Groups" table="fsnote_groups">
    	<id name="id" column="Group_ID">
    	<generator class="native"/>
    	</id>
    	<property name="name"></property>
    </class>
      
</hibernate-mapping>