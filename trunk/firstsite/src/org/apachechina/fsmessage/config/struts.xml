<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts> 
 	<!-- todo: move to /${moduleName}/config/struts.xml文件中 -->
   <package name="message" namespace="/message"  extends="fsmessage" >
        <action name="index" class="org.apachechina.fsmessage.action.List">
            <result>index.jsp</result>
        </action>    
        <action name="add" class="org.apachechina.fsmessage.action.Add">
            <result>Add.jsp</result>
        </action>  
        <action name="addBack" class="org.apachechina.fsmessage.action.Save" >
            <result type="redirectAction">index</result> 
        </action>      
    </package>
   
</struts>
